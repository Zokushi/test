generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" 
  url      = "file:./dev.db"
}

model Location {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  street      String?
  city        String?
  state       String?
  country     String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now()) @updatedAt
}